{
  "name": "KloutKast",
  "displayName": "KloutKast",
  "icon": "./src/assets/images/icon.png",
  "scheme": "myapp",
  "userInterfaceStyle": "automatic",
  "splash": {
    "image": "./src/assets/images/splash.png",
    "resizeMode": "contain",
    "backgroundColor": "#ffffff"
  },
  "orientation": "portrait",
  "updates": {
    "fallbackToCacheTimeout": 0
  },
  "assetBundlePatterns": [
    "**/*"
  ],
  "ios": {
    "supportsTablet": true,
    "bundleIdentifier": "com.dragon.kloutkast"
  },
  "web": {
    "favicon": "./src/assets/images/favicon.png"
  },
  "description": "",
  "apiEndpoint": {
    "local": "http://35.232.226.9/api/v1",
    "dev": "http://35.232.226.9/api/v1"
  },
  "packagerOpts": {
    "config": "metro.config.js",
    "sourceExts": [
      "expo.ts",
      "expo.tsx",
      "expo.js",
      "expo.jsx",
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "wasm",
      "svg"
    ]
  },
  "android": {
    "package": "com.dragon.kloutkast"
  }
}
